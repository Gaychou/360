<krpano version="1.16" title="" onstart="startup();">


	<!-- set skin settings: bingmaps? gyro? thumbnail controlling? tooltips? -->
	

	<!-- set optional skin logo url -->
	<layer name="skin_logo" url="" scale="0.25" opened_onclick="openurl('...',_blank);" />


	<action name="startup">
		if(startscene === null, copy(startscene,scene[0].name));
		loadscene(get(startscene), null, MERGE);
	</action>

	
	<scene name="scene_1" title="玄关" onstart="" thumburl="panos/1.tiles/thumb.jpg" lat="" lng="" heading="">

		<view hlookat="-125.515" vlookat="3.273" fovtype="MFOV" fov="120.000" maxpixelzoom="2.0" fovmin="70" fovmax="140" limitview="auto" />

		<preview url="images/panos/1.tiles/preview.jpg" />

		<image>
			<cube url="images/panos/1.tiles/_%s.jpg" />
			<mobile>
				<cube url="images/panos/1.tiles/_%s.jpg" />
			</mobile>
		</image>


	</scene>

	

</krpano>
